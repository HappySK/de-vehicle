pipeline {
    agent any
    stages {
        stage('Deploy to Airflow') {
            steps {
                sh 'echo Inside Airflow Deployment'
                sh 'pwd'
                sh 'scp -r /var/lib/jenkins/workspace/ine_Vehicle_Data_Analysis_master/dags airflow_user@airflow:/home/airflow_user/airflow/dags'
            }
        }
        stage('Execute DAG') {
            steps {
                sh 'echo Inside DAG Execution'
            }
        }
        stage('Visualize Data') {
            steps {
                sh 'echo Inside Data Visualization'
            }
        }
    }
}